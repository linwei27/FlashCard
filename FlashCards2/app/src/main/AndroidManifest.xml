<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chenke.flashcards">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/weibo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/weibo"
        android:supportsRtl="true"
        android:theme="@style/AppCompatTheme">
        <!--
        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/background_login" />
        -->
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TabFirstActivity" />
        <activity android:name=".TabSecondActivity" />
        <activity android:name=".TabThirdActivity" />
        <activity android:name=".TabFourthActivity" />
        <activity android:name=".userinfo.Login"/>
        <activity android:name=".userinfo.Register"/>
        <activity android:name=".userinfo.UserCenter"/>
        <activity android:name=".study.NumberStudy"/>
        <activity android:name=".study.ShapeStudy"/>
        <activity android:name=".study.ColorStudy"/>
    </application>

</manifest>